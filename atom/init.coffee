atom.commands.add '.platform-darwin atom-text-editor', 'custom:toggle-line-numbers', ->
  atom.config.toggle('editor.showLineNumbers')

atom.workspaceView.command 'custom:close-other-panes', ->
  panes = atom.workspace.getPanes()
  activePane = atom.workspace.getActivePane()
  # maybe have it kill the find-and-replace bar too
  for pane in panes
    unless activePane == pane
      items = pane.getItems()
      for item in items
        path = item.getURI()
        activePaths = activePane.getItems().map (i) -> i.getURI?()
        if activePaths.indexOf(path) == -1
          pane.moveItemToPane(item, activePane, activePane.getItems().length)
      pane.destroy()

atom.workspace.observeTextEditors (editor) ->
  editor.onWillInsertText (edit) ->
    edit.cancel() if edit.text == 'ïœ‡'

